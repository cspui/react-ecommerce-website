{
  "hosting": {
    "public": "/functions/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "function": "app"
    } ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix $Env:RESOURCE_DIR run lint"
    ]
  }
}
